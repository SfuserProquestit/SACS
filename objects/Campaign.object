    <fields>
        <fullName>Limit_Level__c</fullName>
        <externalId>false</externalId>
        <formula>IMAGE(
 IF( Remaining_Seats__c = 0, &quot;/img/samples/light_red.gif&quot;,  
IF( Maximum_Limit__c =  Remaining_Seats__c , &quot;/img/samples/light_green.gif&quot;, &quot;/img/samples/light_yellow.gif&quot;) ) ,
&quot; &quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Campaign Limit</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Maximum_Limit__c</fullName>
        <description>When type School Tour is selected it should display 15</description>
        <externalId>false</externalId>
        <label>Maximum Limit</label>
        <precision>2</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Name</fullName>
    </fields>
    <fields>
        <fullName>No_of_People_Attending__c</fullName>
        <externalId>false</externalId>
        <label>No. of People Attending</label>
        <summarizedField>CampaignMember.No_of_People_Attending__c</summarizedField>
        <summaryFilterItems>
            <field>CampaignMember.Status</field>
            <operation>equals</operation>
            <value>Will Attend</value>
        </summaryFilterItems>
        <summaryForeignKey>CampaignMember.CampaignId</summaryForeignKey>
        <summaryOperation>sum</summaryOperation>
        <type>Summary</type>
    </fields>
    <fields>
        <fullName>NumberOfContacts</fullName>
    </fields>
    <fields>
        <fullName>NumberOfConvertedLeads</fullName>
    </fields>
    <fields>
        <fullName>NumberOfLeads</fullName>
    </fields>
    <fields>
        <fullName>NumberOfOpportunities</fullName>
    </fields>
    <fields>
        <fullName>NumberOfResponses</fullName>
    </fields>
    <fields>
        <fullName>NumberOfWonOpportunities</fullName>
    </fields>
    <fields>
        <fullName>NumberSent</fullName>
    </fields>
