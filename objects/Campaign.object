        <fullName>OwnerId</fullName>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>ParentId</fullName>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Remaining_Seats__c</fullName>
        <externalId>false</externalId>
        <formula>Maximum_Limit__c -  No_of_People_Attending__c</formula>
        <label>Remaining Seats</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Response_Status__c</fullName>
        <externalId>false</externalId>
        <formula>IMAGE(IF (NumberOfResponses   &gt;= 30, &quot;/img/samples/flag_red.gif&quot;, &quot;/img/samples/flag_green.gif&quot;), &quot;Response Status&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Response Status</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Stake_Holder__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>Stake Holder</label>
        <referenceTo>Account</referenceTo>
        <relationshipLabel>Campaigns</relationshipLabel>
        <relationshipName>Campaigns</relationshipName>
        <required>false</required>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>StartDate</fullName>
    </fields>
    <fields>
        <fullName>Status</fullName>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Total_Member__c</fullName>
        <externalId>false</externalId>
        <formula>NumberOfContacts  +  (  NumberOfLeads  -   NumberOfConvertedLeads  )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total Member</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Total_Number_of_Campaign_Members__c</fullName>
        <externalId>false</externalId>
        <label>Total Number of Campaign Members</label>
        <summaryFilterItems>
            <field>CampaignMember.Status</field>
            <operation>equals</operation>
            <value>Attended, Did not attend, Opt Out, Responded, Sent, Will Attend</value>
        </summaryFilterItems>
        <summaryForeignKey>CampaignMember.CampaignId</summaryForeignKey>
        <summaryOperation>count</summaryOperation>
        <type>Summary</type>
    </fields>
    <fields>
        <fullName>Tour_School_Leaders__c</fullName>
        <description>enter the names and year group of the nominated school leaders on the tour</description>
        <externalId>false</externalId>
        <inlineHelpText>enter the names and year group of the nominated school leaders on the tour</inlineHelpText>
        <label>Tour School Leaders</label>
        <length>250</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Tour_Speaker__c</fullName>
        <description>Name of speaker, i.e. Dr Collier, David Smith, Richard Ford</description>
        <externalId>false</externalId>
        <label>Tour Speaker</label>
        <length>155</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Type</fullName>
        <type>Picklist</type>
    </fields>
