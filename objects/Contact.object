    <validationRules>
        <fullName>Arrival_yr_and_Visa_Class_no_mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(Overseas_Student__c = true ,  ISPICKVAL( Resident_of_Australia__c , &quot;Temporary&quot;),  ISPICKVAL( Resident_of_Australia__c, &quot;Permanent&quot;) ) , OR( ISBLANK(What_year_did_he_she_arrive_in_Australia__c ) ,ISBLANK(Visa_Class_No__c )) )</errorConditionFormula>
        <errorMessage>Either Overseas Student or Resident status is selected then it is mandatory to put &quot;Arrival Year in Australia&quot; &amp; &quot;Visa Class No&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Either_Australian_Citizen_or_Overseas_St</fullName>
        <active>true</active>
        <description>When saving a record you need to select either Australian Citizen	or Overseas Student to save the record.</description>
        <errorConditionFormula>IF( RecordTypeId ==&apos;01290000000WKhJ&apos;, OR( AND( Overseas_Student__c , Australian_Citizen__c ) , 
       AND(NOT(Overseas_Student__c) , NOT(Australian_Citizen__c)) ), false)</errorConditionFormula>
        <errorMessage>Select either Australian Citizen	or Overseas Student</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>If_Alumni_mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>IF( Alumni__c , ( ISBLANK( If_Alumni__c ) ) , false)</errorConditionFormula>
        <errorMessage>&quot;If Alumni&quot; is mandatory when Alumni is selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>If_Christian_put_Church_Affiliation</fullName>
        <active>false</active>
        <errorConditionFormula>IF( ISPICKVAL( Religion__c , &apos;Christian&apos;) ,  ISBLANK( Church_Affiliation__c )  ,  IF( NOT(ISPICKVAL( Religion__c , &apos;Christian&apos;)) ,NOT(ISBLANK( Church_Affiliation__c )) , false) )</errorConditionFormula>
        <errorDisplayField>Church_Affiliation__c</errorDisplayField>
        <errorMessage>Mandatory to put Church Affiliation once you have selected Religion as Christian</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>If_Guardian_put_relationship</fullName>
        <active>false</active>
        <errorConditionFormula>IF( ISPICKVAL( Parent_Type__c , &apos;Guardian&apos;) ,  ISBLANK( Relationship_with_Student__c ), IF( NOT(ISPICKVAL( Parent_Type__c , &apos;Guardian&apos;)) ,NOT(ISBLANK( Relationship_with_Student__c )) , false) )</errorConditionFormula>
        <errorDisplayField>Relationship_with_Student__c</errorDisplayField>
        <errorMessage>If Guardian Parent Type is selected it is mandatory to put the relationship with student</errorMessage>
    </validationRules>
