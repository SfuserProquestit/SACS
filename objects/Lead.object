    <validationRules>
        <fullName>For_Web_and_Marketing_Efforts_Campaigns</fullName>
        <active>true</active>
        <errorConditionFormula>IF( OR( ISPICKVAL( LeadSource ,&quot;Web&quot;),ISPICKVAL( LeadSource ,&quot;Marketing Efforts &amp; Campaingns&quot;) ) ,ISBLANK( TEXT( Lead_Sub_Source__c ) ) , false)</errorConditionFormula>
        <errorMessage>Please select the correct Picklist from Lead Sub Source.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>If_Alumni_mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>IF( Alumni__c , ( ISBLANK( If_Alumni__c ) ) , false)</errorConditionFormula>
        <errorDisplayField>If_Alumni__c</errorDisplayField>
        <errorMessage>&quot;If Alumni&quot; is mandatory when Alumni is selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>School_Tour_Date_mandatory</fullName>
        <active>true</active>
        <description>School Tour Date is mandatory when the lead stage &quot;School Tour Requested&quot; is selected</description>
        <errorConditionFormula>ISPICKVAL(  Lead_Stage__c  , &quot;School Tour Requested&quot;) &amp;&amp; ISBLANK(Tour_Date__c ) &amp;&amp;
 ISBLANK( TEXT( School_Tour_Date__c )  )</errorConditionFormula>
        <errorMessage>Please update school tour date</errorMessage>
    </validationRules>
