    <validationRules>
        <fullName>For_Offer_to_Enrolment</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ISPICKVAL( StageName , &quot;Offer&quot;) , ( ISBLANK(   Enrolment_Date__c  ) ) ,false)</errorConditionFormula>
        <errorMessage>If stage &quot;Offer&quot; is selected it is mandatory to put correct &quot;Enrolment Date&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>For_Opp_Interview_Complete</fullName>
        <active>true</active>
        <description>When stage Interview complete is selected, then it is mandatory to put interview date &amp; time</description>
        <errorConditionFormula>AND( ISPICKVAL( StageName , &quot;Interview Complete&quot;), OR( ISBLANK(Interview_Time__c), ISBLANK( Interview_Date__c ) ) )</errorConditionFormula>
        <errorMessage>If &quot;Interview Complete&quot; stage is selected it is mandatory to update Interview date &amp; Interview time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>For_Opp_Stage_Future_Student</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL( StageName , &quot;Future Student&quot;), ISBLANK( Future_Enrolment_Date__c ) )</errorConditionFormula>
        <errorMessage>If &quot;Future Student&quot; Stage is selected then it is mandatory to put Enrolment Term</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>For_Opp_Stage_Interview</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL( StageName , &quot;Offer to Interview&quot;), OR( ISBLANK(Interview_Time__c), ISBLANK( Interview_Date__c ) ) )</errorConditionFormula>
        <errorMessage>If &quot;Offer to Interview&quot; stage is selected it is mandatory to update Interview date &amp; Interview time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>For_Stage_Enrolled</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ISPICKVAL( StageName , &quot;Enrolled&quot;) , OR( ISBLANK( TEXT( School_Grade__c ) ), ISBLANK( TEXT( Term__c ) ) , ISBLANK( TEXT( Joining_Year__c ) ) ) ,false)</errorConditionFormula>
        <errorMessage>If Stage &quot;Enrolled&quot; is selected it is mandatory to select School Grade, Term &amp; Joining Year</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Future_Enrollment_Date</fullName>
        <active>true</active>
        <errorConditionFormula>Future_Enrolment_Date__c &lt;=  TODAY()</errorConditionFormula>
        <errorDisplayField>Future_Enrolment_Date__c</errorDisplayField>
        <errorMessage>Future Enrollment Date cannot be less than or equal to today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SACS_Key_Contact_Is_Other_filled_SACS_Ot</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( SACS_Key_Contact__c , &quot;Other&quot;) &amp;&amp; ISBLANK( SACS_Others__c )</errorConditionFormula>
        <errorDisplayField>SACS_Others__c</errorDisplayField>
        <errorMessage>Please enter SACS Others Details</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Student_needs_to_be_from_same_family</fullName>
        <active>false</active>
        <errorConditionFormula>Student_Name__r.AccountId &lt;&gt;  AccountId</errorConditionFormula>
        <errorDisplayField>Student_Name__c</errorDisplayField>
        <errorMessage>Student needs to be from the same family</errorMessage>
    </validationRules>
</CustomObject>
